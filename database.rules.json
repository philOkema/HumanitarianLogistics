{
    "rules": {
      "locations": {
        "$uid": {
          ".read": "auth != null && auth.uid === $uid",
          ".write": "auth != null && auth.uid === $uid"
        }
      },
      "deliveries": {
        "$deliveryId": {
          ".read": "auth != null",
          ".write": "auth != null"
        }
      }
    }
  }